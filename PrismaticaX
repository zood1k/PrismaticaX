-- Settings theme dropdown
SettingsSection:AddDropdown("Theme", {
    Title = "Interface theme",
    Description = "Select UI theme",
    Values = {"Dark", "Light", "Aqua", "Darker", "Amethyst", "Rose"},
    Default = "Dark",
    Callback = function(val)
        Fluent:SetTheme(val)
    end
})

-- Clear button
ClearSection:AddButton({
    Title = "Clear",
    Description = "Clear all visuals",
    Callback = function()
        EffectManager:ClearVisual()
        for name, toggle in pairs(toggles) do
            if name ~= "Speed" and name ~= "Jump" and name ~= "InfiniteJump" and name ~= "Noclip" and name ~= "Fly" and name ~= "HighlightESP" and name ~= "NameESP" and name ~= "DistanceESP" and name ~= "Aimbot" and name ~= "Spinbot" then
                toggle:SetValue(false)
            end
        end
    end
})

-- Player modification toggles and sliders
toggles.Speed = PlayerSection:AddToggle("Speed", {Title = "Speed", Default = false})
toggles.Speed:OnChanged(function(val)
    if val then
        if not speedConnection then
            speedConnection = RunService.Heartbeat:Connect(function()
                local char = player.Character
                local humanoid = char and char:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.WalkSpeed = CurrentWalkSpeed
                end
            end)
        end
    else
        if speedConnection then
            speedConnection:Disconnect()
            speedConnection = nil
        end
        local char = player.Character
        local humanoid = char and char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = OriginalWalkSpeed
        end
    end
end)

local SpeedSlider = PlayerSection:AddSlider("SpeedSlider", {
    Title = "Speed changer",
    Description = "Set walk speed (16-100)",
    Default = 16,
    Min = 16,
    Max = 100,
    Rounding = 0,
    Callback = function(val)
        CurrentWalkSpeed = val
    end
})

toggles.Jump = PlayerSection:AddToggle("Jump", {Title = "Jump", Default = false})
toggles.Jump:OnChanged(function(val)
    if val then
        if not jumpConnection then
            jumpConnection = RunService.Heartbeat:Connect(function()
                local char = player.Character
                local humanoid = char and char:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.UseJumpPower = true
                    humanoid.JumpPower = CurrentJumpPower
                end
            end)
        end
    else
        if jumpConnection then
            jumpConnection:Disconnect()
            jumpConnection = nil
        end
        local char = player.Character
        local humanoid = char and char:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.UseJumpPower = OriginalUseJumpPower
            if OriginalUseJumpPower then
                humanoid.JumpPower = OriginalJumpPower
            else
                humanoid.JumpHeight = OriginalJumpHeight
            end
        end
    end
end)

local JumpSlider = PlayerSection:AddSlider("JumpSlider", {
    Title = "Jump changer",
    Description = "Set jump power (50-100)",
    Default = 50,
    Min = 50,
    Max = 100,
    Rounding = 0,
    Callback = function(val)
        CurrentJumpPower = val
    end
})

toggles.InfiniteJump = PlayerSection:AddToggle("InfiniteJump", {Title = "Infinite jump", Default = false})
toggles.InfiniteJump:OnChanged(function(val)
    InfiniteJumpEnabled = val
end)

toggles.Noclip = PlayerSection:AddToggle("Noclip", {Title = "Noclip", Default = false})
toggles.Noclip:OnChanged(function(val)
    NoclipEnabled = val
    if val then
        if not noclipConnection then
            noclipConnection = RunService.Stepped:Connect(function()
                if NoclipEnabled and player.Character then
                    for _, part in pairs(player.Character:GetDescendants()) do
                        if part:IsA("BasePart") and part.CanCollide then
                            part.CanCollide = false
                        end
                    end
                end
            end)
        end
    else
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
        if player.Character then
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
    end
end)

toggles.Fly = PlayerSection:AddToggle("Fly", {Title = "Fly", Default = false})
toggles.Fly:OnChanged(function(val)
    FlyEnabled = val
    if val then
        if not flyConnection then
            flyConnection = RunService.Heartbeat:Connect(function()
                if not FlyEnabled then return end
                local char = player.Character
                if not char then return end
                local root = char:FindFirstChild("HumanoidRootPart")
                if not root then return end
                if not flyBodyVelocity or not flyBodyVelocity.Parent then
                    flyBodyVelocity = Instance.new("BodyVelocity")
                    flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
                    flyBodyVelocity.MaxForce = Vector3.new(1e9, 1e9, 1e9)
                    flyBodyVelocity.Parent = root
                end
                local camera = workspace.CurrentCamera
                local direction = Vector3.new()
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    direction = direction + camera.CFrame.LookVector * FlySpeed
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    direction = direction - camera.CFrame.LookVector * FlySpeed
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    direction = direction - camera.CFrame.RightVector * FlySpeed
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    direction = direction + camera.CFrame.RightVector * FlySpeed
                end
                flyBodyVelocity.Velocity = direction
            end)
        end
    else
        if flyConnection then
            flyConnection:Disconnect()
            flyConnection = nil
        end
        if flyBodyVelocity then
            pcall(function() flyBodyVelocity:Destroy() end)
            flyBodyVelocity = nil
        end
    end
end)

local FlySpeedSlider = PlayerSection:AddSlider("FlySpeedSlider", {
    Title = "Fly speed",
    Description = "Set fly speed (1-100)",
    Default = 50,
    Min = 1,
    Max = 100,
    Rounding = 0,
    Callback = function(val)
        FlySpeed = val
    end
})

-- Infinite Jump handler
UserInputService.JumpRequest:Connect(function()
    if InfiniteJumpEnabled and player.Character then
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            pcall(function() humanoid:ChangeState("Jumping") end)
        end
    end
end)

-- Handle window close to cleanup all features
Window:OnClose(function()
    CleanupAll()
end)

-- Cleanup on player leaving
game:GetService("Players").PlayerRemoving:Connect(function(leavingPlayer)
    if leavingPlayer == player then
        CleanupAll()
    else
        if espPlayers[leavingPlayer] then
            if espPlayers[leavingPlayer].Highlight then
                pcall(function() espPlayers[leavingPlayer].Highlight:Destroy() end)
            end
            espPlayers[leavingPlayer] = nil
        end
        if nameEspPlayers[leavingPlayer] then
            if nameEspPlayers[leavingPlayer].BillboardGui then
                pcall(function() nameEspPlayers[leavingPlayer].BillboardGui:Destroy() end)
            end
            nameEspPlayers[leavingPlayer] = nil
        end
        if distanceEspPlayers[leavingPlayer] then
            if distanceEspPlayers[leavingPlayer].BillboardGui then
                pcall(function() distanceEspPlayers[leavingPlayer].BillboardGui:Destroy() end)
            end
            if distanceEspPlayers[leavingPlayer].Connection then
                pcall(function() distanceEspPlayers[leavingPlayer].Connection:Disconnect() end)
            end
            distanceEspPlayers[leavingPlayer] = nil
        end
        if lockedTarget == leavingPlayer then
            lockedTarget = nil
        end
    end
end)

-- Initial setup
Window:SelectTab(1)
